#!/bin/bash

clear

g++		./server.cc \
		./SuperFile.cc \
		../File/TFile.cc \
		../File/TWinFile.cc \
		../Net/TNet.cc \
		../File/platforms/win_file/WinFile.cc \
-l ws2_32 \
-o ../bin/server.exe

#../bin/server.exe

